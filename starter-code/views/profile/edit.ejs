<form action="/profile/<%= detail._id %>" method="POST">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" id="name" name="name" class="form-control" value="<%= detail.name %>">
  </div>
  <div class="form-group">
    <label for="occupation">Username</label>
    <input type="text" name="username" id="username" class="form-control" value="<%= detail.username %>">
  </div>

  <div class="form-group">
    <label for="mail">Email</label>
    <input type="text" name="mail" id="mail" value="<%= detail.email %>" class="form-control">
  </div>

  <div class="form-group">
    <label for="summary">Summary</label>
    <input type="text" name="summary" id="summary" class="form-control" value="<%= detail.summary %>">
  </div>
  <div class="form-group">
    <label for="" class="label-image">Image</label>
    <img src="https://upload.wikimedia.org/wikipedia/en/a/aa/Bart_Simpson_200px.png" alt="" width="100">
  </div>
  <div class="form-group">
    <label for="company">Company</label>
    <input type="text" name="company" id="company" class="form-control" value="<%= detail.company %>">
  </div>
  <div class=" form-group ">
    <label for="jobTitle ">Job Title</label>
    <input type="text" name="jobTitle" id="jobTitle" class="form-control" value="<%= detail.jobTitle %>">
  </div>

  <button type="submit " class="btn btn-primary ">Save</button>
</form>
