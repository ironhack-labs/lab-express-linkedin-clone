<div id="container">
  <a href="/post/new">New Post</a>
  <a href="/logout">Logout</a>

  <h2><%= username %> post</h2>

  <% post.forEach(function(post) { %>
    <div class="post-container">
      <p><%= post.post %></p>
      <p class="date"><%= moment(post.created_at).format("LL, LTS") %></p>
    </div>
  <% }) %>
</div>
